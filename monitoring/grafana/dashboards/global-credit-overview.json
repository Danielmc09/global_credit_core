{
  "id": null,
  "uid": "global-credit-overview",
  "title": "Global Credit - Overview",
  "tags": [
    "credit",
    "applications"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 0,
  "refresh": "10s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "5s",
      "10s",
      "30s",
      "1m",
      "5m"
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "HTTP Requests Total",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(http_requests_total) or vector(0)",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ],
          "fields": ""
        }
      }
    },
    {
      "id": 2,
      "title": "Process Memory Usage (MB)",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "expr": "process_resident_memory_bytes{job='credit-backend-api'} / 1024 / 1024",
          "refId": "A",
          "legendFormat": "Memory"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ],
          "fields": ""
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "decmbytes"
        }
      }
    },
    {
      "id": 3,
      "title": "HTTP Request Rate (requests/sec)",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "rate(http_requests_total{job='credit-backend-api'}[1m]) or vector(0)",
          "refId": "A",
          "legendFormat": "{{method}} {{endpoint}}"
        }
      ],
      "options": {
        "tooltip": {
          "mode": "multi"
        },
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          },
          "unit": "reqps"
        }
      }
    },
    {
      "id": 4,
      "title": "HTTP Request Duration (p95)",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job='credit-backend-api'}[5m])) or vector(0)",
          "refId": "A",
          "legendFormat": "p95 {{method}} {{endpoint}}"
        }
      ],
      "options": {
        "tooltip": {
          "mode": "multi"
        },
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          },
          "unit": "s"
        }
      }
    },
    {
      "id": 5,
      "title": "Python GC Collections",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "targets": [
        {
          "expr": "rate(python_gc_collections_total{job='credit-backend-api'}[1m])",
          "refId": "A",
          "legendFormat": "Gen {{generation}}"
        }
      ],
      "options": {
        "tooltip": {
          "mode": "multi"
        },
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "id": 6,
      "title": "Process CPU Time",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      },
      "targets": [
        {
          "expr": "rate(process_cpu_seconds_total{job='credit-backend-api'}[1m])",
          "refId": "A",
          "legendFormat": "CPU Usage"
        }
      ],
      "options": {
        "tooltip": {
          "mode": "single"
        },
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20
          },
          "unit": "percentunit"
        }
      }
    },
    {
      "id": 7,
      "title": "Database Connections",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      },
      "targets": [
        {
          "expr": "db_connections_active{job='credit-backend-api'}",
          "refId": "A",
          "legendFormat": "Active"
        },
        {
          "expr": "db_connections_idle{job='credit-backend-api'}",
          "refId": "B",
          "legendFormat": "Idle"
        }
      ],
      "options": {
        "tooltip": {
          "mode": "multi"
        },
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "id": 8,
      "title": "Worker Task Failure Rate (%)",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 24
      },
      "targets": [
        {
          "expr": "(sum(rate(worker_tasks_total{task_name=\"process_credit_application\",status=\"failure\"}[5m])) / sum(rate(worker_tasks_total{task_name=\"process_credit_application\"}[5m]))) * 100",
          "refId": "A",
          "legendFormat": "Failure Rate"
        }
      ],
      "options": {
        "tooltip": {
          "mode": "single"
        },
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20
          },
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 5,
                "color": "yellow"
              },
              {
                "value": 10,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 9,
      "title": "Worker Tasks Total (Success vs Failure)",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 24
      },
      "targets": [
        {
          "expr": "sum(rate(worker_tasks_total{task_name=\"process_credit_application\",status=\"success\"}[5m]))",
          "refId": "A",
          "legendFormat": "Success"
        },
        {
          "expr": "sum(rate(worker_tasks_total{task_name=\"process_credit_application\",status=\"failure\"}[5m]))",
          "refId": "B",
          "legendFormat": "Failure"
        }
      ],
      "options": {
        "tooltip": {
          "mode": "multi"
        },
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          },
          "unit": "ops"
        }
      }
    },
    {
      "id": 10,
      "title": "Worker Task Duration (p95)",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 32
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(worker_task_duration_seconds_bucket{task_name=\"process_credit_application\"}[5m])))",
          "refId": "A",
          "legendFormat": "p95 Duration"
        }
      ],
      "options": {
        "tooltip": {
          "mode": "single"
        },
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          },
          "unit": "s"
        }
      }
    },
    {
      "id": 11,
      "title": "Worker Tasks In Progress",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 32
      },
      "targets": [
        {
          "expr": "sum(worker_tasks_in_progress)",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ],
          "fields": ""
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    }
  ]
}